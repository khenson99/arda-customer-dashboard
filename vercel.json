{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/cs/accounts/(.*)",
      "dest": "/api/cs/accounts/[id]?id=$1"
    },
    {
      "src": "/api/arda/(.*)",
      "dest": "https://prod.alpha001.io.arda.cards/$1"
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "https://prod.alpha001.io.arda.cards/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
